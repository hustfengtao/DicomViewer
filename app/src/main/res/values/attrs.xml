<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FolderLinearLayout">
        <attr name="expend" format="boolean"/>
        <attr name="animationduration" format="integer"/>
    </declare-styleable>

    <declare-styleable name="DragPhotoView">
        <attr name="enableexit" format="boolean"/>
        <attr name="enabletap" format="boolean"/>
        <attr name="enabledrag" format="boolean"/>
        <attr name="enablezoom" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="BubbleLayout">
        <attr name="background_color" format="color" />
        <attr name="shadow_color" format="color" />
        <attr name="shadow_size" format="dimension" />
        <attr name="radius" format="dimension" />
        <attr name="direction" format="enum">
            <enum name="left" value="1" />
            <enum name="top" value="2" />
            <enum name="right" value="3" />
            <enum name="bottom" value="4" />
        </attr>
        <attr name="offset" format="dimension" />
    </declare-styleable>
</resources>